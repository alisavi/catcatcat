QuadEqu: 	SolvQuadEqu.o SolvEqu.o io.o test.o
	g++ SolvQuadEqu.o SolvEqu.o io.o test.o -o QuadEqu -Wall
	
SolvQuadEqu.o: SolvQuadEqu.cpp
	g++ SolvQuadEqu.cpp -c SolvQuadEqu.o

SolvEqu.o: SolvEqu.cpp
	g++ SolvEqu.cpp -c SolvEqu.o

io.o: io.cpp
	g++ io.cpp -c io.o

test.o: test.cpp
	g++ test.cpp -c test.o
	
clean:
	rm -rf *.o QuadEdu
